<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.OutputTabController">
	<VBox>
		<HBox>
			<padding>
				<Insets top="10" right="10" bottom="10" left="10" />
			</padding>
			<children>
				<Button fx:id="btnList" translateX="10">새로 고침</Button>
				<TextField fx:id="txtSearch" prefWidth="50"
					promptText="이름" translateX="16" />
				<Button fx:id="btnSearch" translateX="18">검 색</Button>
				<Button fx:id="btnDelete" translateX="20">삭 제</Button>
				<Button fx:id="btnSaveFileDir" translateX="22">저장폴더</Button>
				<TextField fx:id="txtSaveFileDir" prefWidth="100"
					promptText="폴더를 선택하세요" translateX="24" />
				<Button fx:id="btnExcel" translateX="29">엑셀파일</Button>

			</children>
		</HBox>
		<padding>
			<Insets top="10" right="10" bottom="10" left="10" />
		</padding>
		<TableView fx:id="tableView" prefHeight="480"
			prefWidth="190" translateX="23">
		</TableView>
	</VBox>
</AnchorPane>

