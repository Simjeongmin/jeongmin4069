<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.OutputTabController">
	<VBox>
		<HBox>
			<padding>
				<Insets top="10" right="10" bottom="10" left="10" />
			</padding>
			<children>
				<Button fx:id="btnList">새로 고침</Button>
				<!-- <TextField fx:id="txtSearch" prefWidth="50"
					promptText="이름" />
				<Button fx:id="btnSearch">검 색</Button> -->
				<Button fx:id="btnDelete">삭 제</Button>
				<Button fx:id="btnSaveFileDir">저장폴더</Button>
				<TextField fx:id="txtSaveFileDir" prefWidth="200"
					promptText="폴더를 선택하세요" />
				<Button fx:id="btnExcel">엑셀파일</Button>

			</children>
		</HBox>
		<padding>
			<Insets top="10" right="10" bottom="10" left="10" />
		</padding>
		<TableView fx:id="tableView" prefHeight="480" prefWidth="150">
		</TableView>
	</VBox>
</AnchorPane>